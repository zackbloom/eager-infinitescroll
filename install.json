{
  "resources": {
    "body": [
      {
        "type": "script",
        "src": "./script.js",
        "moduleType": "global",
        "depends": {
          "github.com/jquery/jquery:dist/jquery.js": "$"
        }
      }
    ]
  },
  "dependencies": {
    "github.com/jquery/jquery": "2.1.4"
  },
  "options": {
    "properties": {
      "container": {
        "title": "Container of the items",
        "type": "string",
        "format": "selector",
        "default": "main ul"
      },
      "next": {
        "title": "Next page button",
        "type": "string",
        "format": "selector",
        "default": "a[rel=\"next\"]"
      }
    }
  }
}
